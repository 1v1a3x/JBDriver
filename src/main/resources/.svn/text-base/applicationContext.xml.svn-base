<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="driverProvider" class="org.jbehave.web.selenium.PropertyWebDriverProvider"/>
    <bean id="properiesUtil" class="com.macys.breeze.jbehave.mass.utils.PropertiesUtil"/>


    <context:component-scan base-package="com.macys.breeze.jbehave.mass.utils"/>
    <context:component-scan base-package="com.macys.breeze.jbehave.mass.pages"/>
    <context:component-scan base-package="com.macys.breeze.jbehave.mass.steps"/>
</beans>